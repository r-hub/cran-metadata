{
    "name": "r-app",
    "postCreateCommand": "service ssh start",
    "dockerComposeFile": "docker-compose.yml",
    "mounts": [{
        "source": "..",
        "target": "/workspace/${localWorkspaceFolderBasename}",
        "type": "bind"
    }],
    "workspaceFolder": "/workspace/${localWorkspaceFolderBasename}",
    "service": "r-app",
    "shutdownAction": "stopCompose"
}
